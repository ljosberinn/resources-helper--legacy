{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn"
  },
  "globals": {
    "crEl": true,
    "google": true,
    "Highcharts": true,
    "sorttable": true,
    "swal": true,
    "tippy": true,
    "getCookie": true,
    "applyTippyOnNewElement": true,
    "toggleTechUpgradeInfo": true,
    "loadingAnimToggler": true,
    "rHelper": true,
    "markerCluster": true,
    "MarkerClusterer": true
  }
}
